<p id="notice"><%= notice %></p>

<h1>Library Zones</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @library_zones.each do |library_zone| %>
      <tr>
        <td><%= library_zone.name %></td>
        <td><%= link_to 'Show', library_zone_path(library_zone) + '.json' %></td>
        <td><%= link_to 'Edit', edit_library_zone_path(library_zone) %></td>
        <td><%= link_to 'Destroy', library_zone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Library Zone', new_library_zone_path %>
